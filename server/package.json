{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "main": "server.js",
    "start": "node index",
    "dev": "nodemon index"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testEnvironment": "node",
    "globals": {
      "NODE_ENV": "test"
    },
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ],
    "testRunner": "jest-circus/runner",
    "testMatch": [
      "**/__tests__/**/*.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(@babel)/)"
    ],
    "verbose": true,
    "testURL": "http://localhost/",
    "extensionsToTreatAsEsm": [ ".jsx", ".ts", ".tsx"]
  },
  "type": "module",
  "keywords": [],
  "author": "Team 39",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.5.0",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.4.0",
    "mongoose": "^7.1.1",
    "mongoose-sequence": "^5.3.1",
    "parser": "^0.1.4",
    "path": "^0.12.7",
    "url": "^0.11.0"
  },
