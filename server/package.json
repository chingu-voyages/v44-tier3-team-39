    "@babel/register": "^7.21.0",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^8.12.2",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  }
}
